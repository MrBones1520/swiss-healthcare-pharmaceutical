model {
    ProductItem productItem
}

json {
    barcode productItem.barcode
    base g.render(productItem.status, [excludes:['status', 'name']]){
        status {
            id productItem.status.id
            name productItem.status.name
        }
    }
    assigned productItem.assigned
    dateCreated productItem.dateCreated
    lastUpdated productItem.dateCreated
}